  "editor.tokenColorCustomizations": {
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "[*Light*]": {
      "textMateRules": [
        {
          "name": "reset colors on most scopes to simple black",
          "scope": [
            "constant",
            "constant.character",
            "constant.character.escape",
            "constant.language",
            "constant.other.option",
            "entity",
            "entity.name",
            "entity.name.function",
            "entity.name.namespace",
            "entity.name.tag",
            "entity.name.type",
            "entity.other.attribute-name",
            "entity.other.inherited-class",
            "keyword",
            "keyword.control",
            "keyword.operator",
            "keyword.operator.logical",
            "keyword.operator.logical.python",
            "keyword.operator.new",
            "markup.bold",
            "markup.heading",
            "markup.inline.raw",
            "markup.fenced_code.block.markdown",
            "meta.definition.variable.name",
            "meta.function-call.arguments",
            "meta.paragraph.markdown",
            "punctuation.definition.tag",
            "punctuation.section.embedded",
            "source",
            "storage.modifier",
            "storage.type",
            "storage.type.groovy",
            "storage.type.object.array.groovy",
            "string",
            "string.quoted.double.xml",
            "string.quoted.single.yaml",
            "string.regexp",
            "string.unquoted.plain.out.yaml",
            "storage.type.generic.groovy",
            "storage.type.primitive.groovy",
            "support.class",
            "support.function",
            "support.type",
            "support.type.property-name.json",
            "support.variable.js",
            "text.xml",
            "variable",
            "variable.language",
            "variable.other.constant"
          ],
          "settings": {
            "fontStyle": "",
            "foreground": "#000"
          }
        },
        {
          "name": "reset styling on many scopes",
          "scope": [
            "invalid.deprecated.shell",
            "keyword.operator",
            "meta.function-call.arguments",
            "meta.function-call punctuation",
            "meta.function-call string.quoted punctuation.definition",
            "punctuation.definition.inheritance.begin.python",
            "punctuation.definition.inheritance.end.python",
            "punctuation.separator",
            "punctuation.separator.key-value"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "name": "reset styling on operators and symbols",
          "scope": [
            "keyword.operator.arithmetic",
            "keyword.operator.assignment",
            "keyword.operator.comparison",
            "keyword.operator"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "name": "reset styling on parentheses, brackets and braces",
          "scope": [
            "punctuation.definition.arguments",
            "punctuation.definition.dict",
            "punctuation.definition.list",
            "punctuation.definition.parameters",
            "punctuation.parenthesis"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "name": "reset styling on key names",
          "scope": [
            "constant.other.key.groovy"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "name": "render language keywords in bold text",
          "scope": [
            "keyword.control",
            "keyword.operator.logical",
            "keyword.operator.logical.python",
            "keyword.operator.new",
            "keyword.other",
            "storage.modifier.static",
            "storage.type",
            "storage.type.class",
            "storage.type.function",
            "storage.type.groovy",
            "storage.type.object",
            "storage.type.object.array",
            "storage.type.object.array.groovy",
            "storage.type.primitive.groovy"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render language constants in bold text",
          "scope": [
            "constant.language"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render named constants in bold text",
          "scope": [
            "constant.other",
            "variable.other.constant"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render YAML anchors and aliases and XML brackets in bold text",
          "scope": [
            "entity.name.type.anchor",
            "punctuation.definition.tag",
            "variable.other.alias"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render constant numbers in colorful bold italics",
          "scope": [
            "constant.numeric"
          ],
          "settings": {
            "fontStyle": "bold italic",
            "foreground": "#ab296a"  // pink-600
          }
        },
        {
          "name": "render interesting single characters in bold color",
          "scope": [
            "keyword.operator.unpacking",
            "punctuation.separator.continuation",
            "storage.type.string.python"
          ],
          "settings": {
            "fontStyle": "bold",
            "foreground": "#ab296a"  // pink-600
          }
        },
        {
          "name": "render YAML anchors in bold color",
          "scope": [
            "constant.language.merge",
            "keyword.control.flow.block-scalar.folded",
            "punctuation.definition.alias",
            "punctuation.definition.anchor"
          ],
          "settings": {
            "fontStyle": "bold",
            "foreground": "#ab296a"  // pink-600
          }
        },
        {
          "name": "render strings and surrounding quotation marks in italics",
          "scope": [
            "punctuation.definition.string",
            // "punctuation.separator",
            // "punctuation.separator.key-value",
            "string.quoted",
            "string.unquoted",
            "string.quoted.double.xml",
            "string.quoted.double.yaml",
            "string.quoted.double.yaml punctuation.definition.string",
            "string.quoted.single.yaml",
            "string.quoted.single.yaml punctuation.definition.string",
            "string.unquoted.plain.out.yaml",
            "text.xml"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "name": "render regular expressions in italics",
          "scope": [
            "string.regexp",
            "meta.function-call.arguments string.regexp"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "name": "render regular expression special characters in bold color",
          "scope": [
            "constant.other.character-class.regexp",
            "constant.other.set.regexp",
            "keyword.control.anchor.regexp",
            "keyword.operator.quantifier.regexp",
            "support.other.escape.special.regexp",
            "support.other.match",
            "support.other.parenthesis.regexp punctuation.parenthesis",
            "keyword.operator.or.regexp",
            "punctuation.definition.group.regexp"
          ],
          "settings": {
            "fontStyle": "bold",
            "foreground": "#ab296a"  // pink-600
          }
        },
        {
          "name": "render docstrings in faint text",
          "scope": [
            "fenced_code.block.language.markdown",
            "string.quoted.docstring",
            "string.quoted.docstring punctuation.definition.string"
          ],
          "settings": {
            "fontStyle": "",
            "foreground": "#6c757d"  // gray-600
          }
        },
        {
          "name": "render comments in faint italics",
          "scope": [
            "comment",
            "punctuation.definition.comment"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#6c757d"  // gray-600
          }
        },
        {
          "name": "render TODO's and FIXME's in bold",
          "scope": "keyword.codetag.notation.python",
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render decorators in bright color",
          "scope": [
            "entity.name.function.decorator",
            "entity.name.function.preprocessor",
            "meta.function.decorator",
            "meta.preprocessor",
            "punctuation.definition.list",
            "punctuation.definition.list.begin.markdown",
            "storage.type.annotation.groovy"
          ],
          "settings": {
            "fontStyle": "",
            "foreground": "#ab296a"  // pink-600
          }
        },
        {
          "name": "render decorator arguments in bold color",
          "scope": [
            "entity.name.function.decorator.arguments",
            "meta.function.decorator.arguments",
            "meta.function.decorator meta.function-call.arguments variable.parameter.function-call",
            "meta.function.decorator meta.function-call.arguments keyword.operator.assignment",
            "meta.function.decorator meta.function-call.arguments string.quoted",
            "meta.function.decorator meta.function-call.arguments string.quoted punctuation.definition.string",
            "meta.function.decorator meta.function.parameters string.quoted",
            "meta.function.decorator meta.function.parameters string.quoted punctuation.definition.string"
          ],
          "settings": {
            "foreground": "#ab296a"  // pink-600
          }
        },
        {
          "name": "render function definitions in bold",
          "scope": [
            "entity.name.function",
            "support.function.magic.python"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render function calls in bold",
          "scope": [
            "meta.function-call",
            "meta.function-call.generic"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render function call arguments in roman text",
          "scope": [
            "meta.function-call meta.function-call.arguments"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "name": "render class definitions in bold",
          "scope": [
            "entity.name.class",
            "entity.name.type.class",
            "meta.implementation",
            "storage.type.generic.groovy",
            "support.class"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render class inheritance in bold italics",
          "scope": [
            "entity.other.inherited-class"
          ],
          "settings": {
            "fontStyle": "bold italic"
          }
        },
        {
          "name": "render bold markdown in bold",
          "scope": [
            "markup.bold"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render markdown italic in italics",
          "scope": [
            "markup.italic"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "name": "render markdown headings in bold",
          "scope": [
            "entity.name.section.markdown",
            "markup.heading"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render markdown hyperlinks in blue",
          "scope": [
            "markup.underline.link",
            "markup.underline.link.comment",
            "markup.underline.link.hover",
            "markup.underline.link.markdown",
            "meta.link.inline.markdown",
            "punctuation.definition.link",
            "string.other.link.title.markdown",
            "string.quoted.double.html"
          ],
          "settings": {
            "fontStyle": "",
            "foreground": "#0a58ca"  // blue-600
          }
        },
        {
          "name": "render variable interpolation in strings in bold",
          "scope": [
            "meta.expression.assignment.shell",
            "punctuation.definition.variable.shell",
            "punctuation.section.embedded",
            "punctuation.definition.variable.shell variable.other.normal.shell"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "render invalid characters in red",
          "scope": [
            "invalid"
          ],
          "settings": {
            "foreground": "#b02a37"  // red-600
          }
        }
      ]
    }
  },
